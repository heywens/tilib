"#lblTitle": {
	textAlign : "center",
	height : Ti.UI.SIZE,
	width : "100%",
	text : "Set date",

	font : {
		fontSize : "18dp",
		fontWeight : "bold",
		fontFamily : "Open Sans"
	}
},

"#lblTitle[platform=android]": {
	font: {
		fontSize : "18dp",
		fontFamily : "OpenSans-Bold"
	}
},

"Label": {
	color : "#FFFFFF",
	height : 30,

	font : {
		fontSize : "16dp",
		fontFamily : "Open Sans"
	}
},

"Label[platform=android]": {
	font: {
		fontSize : "16dp",
		fontFamily : "OpenSans-Regular"
	}
},

".dateLabel": {
	color : "#757373",
	textAlign : "center",
	width : "92%",
	height : Ti.UI.SIZE,

	font : {
		fontSize : "25dp",
		fontFamily : "Open Sans"
	}
},

".dateLabel[platform=android]":{
	font : {
		fontSize : "25dp",
		fontFamily : "OpenSans-Regular"
	}
},

"#winPicker": {
	width : "100%",
	layout : "composite",
	height : "100%",
	opacity : 1,
	navBarHidden : true
},

"#trans": {
	width : "100%",
	height : "100%",
	backgroundColor : "#000000",
	opacity : 0.6
},

"#mView": {
	height : Ti.UI.SIZE,
	width : 300,
	opacity : 1,
	layout : "vertical",
	backgroundColor : "#FFFFFF"
},

"#nav": {
	width : "100%",
	layout : "composite",
	height : 50,
	backgroundColor : "#2B3F4E"
},

"#navBottom": {
	width : "100%",
	layout : "composite",
	height : 50,
	backgroundColor : "#2B3F4E"
},

"#btnCancel": {
	text : "Cancel",
	width : "49%",
	textAlign : "center",
	height : "100%",
	left : 0
},

"#iView1": {
	width : 0.5,
	backgroundColor : "#FFFFFF",
	height : "85%"
},

"#btnDone": {
	text : "Done",
	width : "49%",
	textAlign : "center",
	height : "100%",
	right : 0
},

".arrows" : {
	width : 32,
	height : 34,
	textAlign : "center",
	color : "#363637",
	
	font : {
		fontFamily : "HqTi",
		fontSize : "30dp"
	}
}
