"Label": {
	color : "#FFFFFF",
	
	font:{
		fontFamily: "Open Sans"
	}
},

"Label[platform=android]": {
	font:{
		fontFamily: "OpenSans-Regular"
	}
},

"#lblUrl": {
	color : "#000000",
	
	font:{
		fontFamily: "Open Sans"
	},
	text: "Server URL",
	width: Titanium.UI.FILL,
	top: 30,
	left: 10,
	right: 10
},

"lblUrl[platform=android]": {
	font:{
		fontFamily: "OpenSans-Regular"
	}
},

"#winSetup": {
