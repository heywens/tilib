	width: 300,
	layout: "composite",
	height: 50,
	bottom: 20
},

"#txtAddress1": {
	width: "35%",
	left: 10,
	borderStyle: Titanium.UI.INPUT_BORDERSTYLE_ROUNDED,
	hintText: "(required)"
},

