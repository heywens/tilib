"#mainWindow": {
	width : "100%",
	height : "100%",
	backgroundColor : "#2B3F4E",
	layout : "composite",
	tabBarHidden : true,
	navBarHidden : true,
	fullscreen : false
},

"#menuView": {
	top : 0,
	left : 0,
	width : Ti.UI.SIZE,
	zIndex : 1,
	height : "100%",
	layout : "composite",
	backgroundColor: "#333333"
},

"#menuTable": {
	top : 0,
	backgroundColor: "#333333",
	color: "#ffffff",
	separatorColor: "#2A2A2A",
	minRowHeight : 60,
	height : Ti.UI.FILL	
},

"#contentView": {
	top : 0,
	left : 0,
	zIndex: 3,
	backgroundColor : "#D7DADB",
	layout : "vertical",
	width : "100%",
	height : "100%"
},

"#nav": {
	top: 0,
	backgroundColor : "#2B3F4E",
