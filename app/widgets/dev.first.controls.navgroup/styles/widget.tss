	layout : "composite",
	width : "100%",
	height : 50,
	backgroundColor : "#2B3F4E"
},

"#backButtonTitle": {
	color : "#FFFFFF",
	textAlign : "left",
	
	font : {
		fontSize : "16dp",
		fontFamily : "Open Sans"
	},
	
	left : 23,
	ellipsize : true,
	wordWrap : false
},

"#backButtonTitle[platform=android]": {
	font: {
		fontSize : "16dp",
		fontFamily : "OpenSans-Regular"
	}
},

"#winTitle": {
